{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"ignore": ["./src/routeTree.gen.ts"],
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"noNonNullAssertion": "warn"
			}
		}
	},
	"formatter": {
		"ignore": ["./src/routeTree.gen.ts"],
		"enabled": true,
		"formatWithErrors": false,
		"attributePosition": "auto",
		"indentStyle": "tab",
		"indentWidth": 2,
		"lineWidth": 120
	},
	"json": {
		"formatter": {
			"trailingCommas": "none"
		}
	}
}
